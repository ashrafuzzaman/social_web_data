<h1>Listing profile_attributes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Value</th>
    <th>Type</th>
    <th>Active</th>
    <th>User</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @profile_attributes.each do |profile_attribute| %>
  <tr>
    <td><%= profile_attribute.name %></td>
    <td><%= profile_attribute.value %></td>
    <td><%= profile_attribute.type %></td>
    <td><%= profile_attribute.active %></td>
    <td><%= profile_attribute.user %></td>
    <td><%= link_to 'Show', profile_attribute %></td>
    <td><%= link_to 'Edit', edit_profile_attribute_path(profile_attribute) %></td>
    <td><%= link_to 'Destroy', profile_attribute, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Profile attribute', new_profile_attribute_path %>
